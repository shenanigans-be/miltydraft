{
    "require": {
        "vlucas/phpdotenv": "^5.4",
        "aws/aws-sdk-php": "^3.339",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
      "phpstan": "vendor/bin/phpstan analyse --memory-limit=2G --ansi",
      "paratest": [
        "Composer\\Config::disableProcessTimeout",
        "vendor/bin/paratest -p12 --passthru-php=\"'-d' 'memory_limit=2G'\""
      ]
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "brianium/paratest": "^7.8",
        "fakerphp/faker": "^1.24"
    }
}
